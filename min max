#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a[5]={0,1,2,3,4},b[5],n=5;
    int mn=INT_MAX;
    int mx=INT_MIN;
    for (int i = 0; i < n; i++)
    {
        if(a[i]<mn)
        {
            mn=a[i];
        }
        if(a[i]>mx)
        {
            mx=a[i];
        }
    }
    cout<<mn<<mx;
    
}
